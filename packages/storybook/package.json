{
  "name": "@nl-design-system-candidate/storybook",
  "author": "Community for NL Design System",
  "description": "Storybook based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "type": "module",
  "keywords": [
    "nl-design-system"
  ],
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/candidate.git",
    "directory": "packages/storybook"
  },
  "scripts": {
    "build": "storybook build --config-dir ./config/ --output-dir ./dist/",
    "clean": "rimraf ./dist/",
    "dev": "pnpm run storybook",
    "storybook": "storybook dev --config-dir ./config/ --port 6006"
