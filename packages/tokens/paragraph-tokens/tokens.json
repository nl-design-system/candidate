{
  "nl": {
    "paragraph": {
      "color": {
        "$type": "color",
        "$extensions": {
          "nl.nldesignsystem.css-property-syntax": "<color>",
          "nl.nldesignsystem.figma-implementation": true
        }
      },
      "font-family": {
        "$type": "fontFamilies",
        "$extensions": {
          "nl.nldesignsystem.css-property-syntax": ["<family-name>", "<generic-name>"],
          "nl.nldesignsystem.figma-implementation": true
        }
      },
      "font-size": {
        "$type": "fontSizes",
        "$extensions": {
          "nl.nldesignsystem.css-property-syntax": ["<length>", "<percentage>"],
          "nl.nldesignsystem.figma-implementation": true
        }
      },
      "font-weight": {
        "$type": "fontWeights",
        "$extensions": {
          "nl.nldesignsystem.css-property-syntax": "<number>",
          "nl.nldesignsystem.figma-implementation": true
        }
      },
      "line-height": {
        "$type": "lineHeights",
        "$extensions": {
          "nl.nldesignsystem.css-property-syntax": ["<length>", "<number>"],
          "nl.nldesignsystem.figma-implementation": true
        }
      },
      "lead": {
        "font-size": {
          "$type": "fontSizes",
          "$extensions": {
            "nl.nldesignsystem.css-property-syntax": ["<length>", "<percentage>"],
            "nl.nldesignsystem.figma-implementation": true
          }
        },
        "font-weight": {
          "$type": "fontWeights",
          "$extensions": {
            "nl.nldesignsystem.css-property-syntax": "<number>",
            "nl.nldesignsystem.figma-implementation": true
          }
        },
        "line-height": {
          "$type": "lineHeights",
          "$extensions": {
            "nl.nldesignsystem.css-property-syntax": ["<length>", "<number>"],
            "nl.nldesignsystem.figma-implementation": true
          }
        }
      }
    }
  }
}
