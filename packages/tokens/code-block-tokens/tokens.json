{
  "nl": {
    "code-block": {
      "padding-block": {
        "$type": "spacing",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": "<length>",
            "inherits": false
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "padding-inline": {
        "$type": "spacing",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": "<length>",
            "inherits": false
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "background-color": {
        "$type": "color",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": "<color>",
            "inherits": true
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "color": {
        "$type": "color",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": "<color>",
            "inherits": true
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "font-family": {
        "$type": "fontFamilies",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": ["<family-name>", "<generic-name>"],
            "inherits": true
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "line-height": {
        "$type": "lineHeights",
