          "nl.nldesignsystem.css-property": {
            "syntax": ["<length>", "<number>"],
            "inherits": true
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "font-size": {
        "$type": "fontSizes",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": ["<length>", "<percentage>"],
            "inherits": true
          },
          "nl.nldesignsystem.figma-available": true
        }
      },
      "border-radius": {
        "$type": "borderRadius",
        "$extensions": {
          "nl.nldesignsystem.css-property": {
            "syntax": "<length-percentage>",
            "inherits": false
          },
          "nl.nldesignsystem.figma-available": true
        }
      }
    }
  }
}
